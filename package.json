{
  "name": "salhouse-goldens",
  "version": "1.0.0",
  "description": "Salhouse Goldens",
  "main": "index.js",
  "author": "P R McNie",
  "license": "ISC",
  "scripts": {
    "format": "npx eslint . --fix --ext .js,.jsx",
    "lint": "npx eslint . --ext .js,.jsx --ignore-path .eslintignore",
    "build": "webpack --config client/webpack/webpack.prod.config.js",
    "dev:build": "webpack --config client/webpack/webpack.dev.config.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "webpack --config client/webpack/webpack.dev.config.js --watch",
    "dev:server": "nodemon server",
    "start": "node server",
    "test": "jest --verbose --noStackTrace"
  },
  "engines": {
    "node": "12.x"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/tests/cypress/"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.4.4",
    "bootstrap": "^4.5.2",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "react-bootstrap": "^1.3.0",
    "style-loader": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "cypress": "^5.0.0",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "webpack": "^5.0.0-rc.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
